Algoritmo area
	//definir codifo
	Definir areaDelRectangulo, base, altura Como real;
	//tomar datos
	Escribir "digite la base del rectangulo:";
	Leer base
	Escribir "digite la altura del rectangulo:";
	Leer altura
	//mostrar resultado
	areaDelRectangulo <- base * altura;
	Escribir " el area del rectangulo es:", areaDelRectangulo;
	
	
	
FinAlgoritmo
